{
  "name": "@sekuire/google-workspace-agent",
  "version": "1.0.0",
  "description": "Sekuire AI Agent for Google Workspace - Create, edit, and manage Google Docs",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome lint src/",
    "format": "biome format src/ --write",
    "auth": "tsx scripts/generate-token.ts"
  },
  "dependencies": {
    "@sekuire/sdk": "^0.1.11",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "googleapis": "^144.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=22"
  }
}
